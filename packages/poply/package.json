{
  "name": "poply",
  "description": "Poply is lightweight toast library for React",
  "repository": "sveenxx/poply",
  "version": "3.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "size": "size-limit"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@poply/eslint-config-poply": "workspace:*",
    "@poply/tailwind-config": "workspace:*",
    "@poply/tsconfig": "workspace:*",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "size-limit": "8.2.4",
    "tsup": "6.6.3",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "packageManager": "pnpm@7.17.0",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/poply.mjs",
      "limit": "5 KB"
    }
  ],
  "dependencies": {
    "goober": "^2.1.12"
  }
}
